


<!DOCTYPE html>
	<html>	
		<head>
			<meta charset="utf-8"/>
			<meta name="viewport" content="initial-scale=1, maximum-scale=1, width=device-width" />
			<title>nVisionController</title>	
			<link rel="shortcut icon" type="image/x-icon" href="" /> 
			<link href="style/common.css" rel="stylesheet">
			<link href="style/panels.css" rel="stylesheet">
			
			
		</head>
		<body>
		panels
		
		<%
						
				
			var name =  JSON.parse(panelName);
			
			var inputs = JSON.parse(panelInputs);
			var inputsNb = inputs.length;
			
			var outputs = JSON.parse(panelOutputs);
			var outputsNb = outputs.length;
		%>
		
		<%= inputsNb%>
		<%= outputsNb%>
		<%= name%>
		<%= test%>
		
		<div id="<%=name%>" class="panel" >
		 
			<%
				for (var n=0; n<inputsNb; n++){
			%>
				<div class="panelBtn in" name="in" id="in<%=inputs[n]%>">
					<div name="<%=inputs[n]%>">
						<%=inputs[n]%>
					</div>
				</div>
			<%
				}
			%>
			
			<%
				for (var n=0; n<outputsNb; n++){
			%>
				<div class="panelBtn out" name="out" id="out<%=outputs[n]%>">
					<div name="<%=outputs[n]%>">
						<%=outputs[n]%>
					</div>
				</div>
			<%
				}
			%>
		</div>
		
		
		<script src="/socket.io/socket.io.js"></script>
		<script src="js/panels.js"></script>
		
		<script>
			
		</script>
	
	
		</body>
	</html>